{
  "compilerOptions": {
    /*     "outFile": "./compiled/local/tsc.js",
     */ "outDir": "./compiled/",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6", "dom"],
    "jsx": "react",
    "declaration": true,
    "moduleResolution": "node",
    "types": ["react", "jest", "node"],
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "include": ["./precompile/index.tsx"],
  "exclude": [
    "typings/main",
    "typings/main.d.ts",
    "typings/index.d.ts",
    "node_modules",
    "build",
    "scripts",
    "__tests__/**/*",
    "preDist",
    ".expo",
    "src/setupTests.ts",
    "gulpfile.ts",
    "typings",
    "secrets.js",
    "secrets.ts"
  ]
}
